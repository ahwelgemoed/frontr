{
  "name": "frontr-app",
  "author": "A.H.Welgemoed",
  "description": "An electron App",
  "build": {
    "publish": {
      "provider": "github",
      "releaseType": "release"
    },
    "extraResources": [
      {
        "from": "filestoCopy",
        "to": "data"
      }
    ],
    "appId": "dev.ahwelgemoed.www",
    "mac": {
      "target": "dmg",
      "icon": "./public/icons/mac/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "./public/icons/png/256x256.png"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.162",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.27.32",
    "@types/rc-steps": "^3.5.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-loader": "^2.4.4",
    "@types/react-loader-spinner": "^3.1.1",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-toastify": "^4.1.0",
    "@types/shelljs": "^0.8.8",
    "@types/socket.io-client": "^1.4.34",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^8.3.0",
    "antd": "^4.8.2",
    "axios": "^0.20.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "electron-is-dev": "^1.2.0",
    "framer-motion": "^2.9.4",
    "lodash": "^4.17.20",
    "material-table": "^1.69.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "prettier-plugin-html-template-literals": "^1.0.5",
    "ramda": "^0.27.1",
    "rc-menu": "^8.8.3",
    "react": "^16.14.0",
    "react-detect-offline": "^2.4.0",
    "react-device-detect": "^1.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^3.11.0",
    "react-loader": "^2.4.7",
    "react-loader-spinner": "^3.1.14",
    "react-modal": "^3.11.2",
    "react-query": "^2.23.1",
    "react-query-devtools": "^2.6.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-toastify": "^6.0.9",
    "shelljs": "^0.8.4",
    "socket.io-client": "^2.3.1",
    "styled-components": "^5.2.0",
    "terminal-in-react": "^4.3.1",
    "typescript": "~3.7.2",
    "uuid": "^8.3.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder -w",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build",
    "dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "10.1.3",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.2.0",
    "wait-on": "^5.2.0"
  }
}
